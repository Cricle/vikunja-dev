{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "VikunjaHook.Mcp": "Information"
    }
  },
  "Cors": {
    "AllowedOrigins": [],
    "AllowedMethods": [ "GET", "POST", "PUT", "DELETE" ],
    "AllowedHeaders": [ "Authorization", "Content-Type" ]
  },
  "RateLimit": {
    "Enabled": true,
    "RequestsPerMinute": 30,
    "RequestsPerHour": 500
  }
}
